profiles {
  standard { 
    
      process {
        k8s {
          executor = "k8s"
          cpus = 1
          memory = 1.GB
          pod = [secret: "prp-s3-credentials/credentials", mountPath: "/root/.aws/credentials"]
          namespace = "nextflowucsc"
      }
      aws{ 
      endpoint = "https://s3.nautilus.optiputer.net"
      region = "us-west-2"
      }
    }
  }
}